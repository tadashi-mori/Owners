#login-inner
  %section.form-login01
    %h1 ログイン
    = form_tag("/login") do
      %div{:style => "display: none"}
        %input{:name => "_method", :type => "hidden", :value => "POST"}/
        %input{:id => "", :name => "data[_Token][key]", :type => "hidden", :value => ""}/
      .input
        .input.text
          %input#last-name.jq-placeholder.style01.w300{:name => "email", :value => @email, :placeholder => "メールアドレス", :style => "color: silver;", :type => "string"}/
      .input
        .input.password
          %input#last-name.jq-placeholder.style01.w300{:name => "password", :placeholder => "パスワード", :style => "color: silver;", :type => "password", :value => @password}/
        %label{:style => "font-size: 11px;"}

      %input#signup_submit.submit.w300.green{:name => "", :type => "submit", :value => "ログイン"}/
      %div{:style => "display: none;"}
        %input{:name => "", :type => "hidden"}/
        %input{:name => "", :type => "hidden"}/
